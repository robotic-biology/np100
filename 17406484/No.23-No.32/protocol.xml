        <commandType>COM_FREE-ON_00_0002</commandType>
        <category>&lt;NewCmd_ON&gt;</category>
        <settingParam1 name="Detail">examine</settingParam1>
        <settingParam2 name="Device">(non)</settingParam2>
      </item>
