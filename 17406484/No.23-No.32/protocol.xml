      <item>
        <itemId>11</itemId><type>COMMAND_ADD</type>
        <id>2</id>
        <comment>bring the volume to 4.5 ml with CMF-HBSS.</comment>
        <commandType>COM_ADD_00_0002</commandType>
        <category>ADD</category>
        <settingParam1 name="Reagent">CMF-HBSS</settingParam1>
        <settingParam2 name="Reagent quantity">0</settingParam2>
